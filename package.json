{
  "name": "herit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "db:generate": "drizzle-kit generate",
    "db:generate:safe": "npx tsx scripts/generate-migration.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:safe": "npx tsx scripts/migrate.ts",
    "db:migrate:dev": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:migrate:staging": "drizzle-kit migrate --config=drizzle-staging.config.ts",
    "db:migrate:prod": "drizzle-kit migrate --config=drizzle-prod.config.ts",
    "db:sync": "npx tsx scripts/sync-schema.ts",
    "db:push": "echo 'DEPRECATED: Use db:migrate instead for safe migrations'",
    "db:reset": "npx tsx scripts/reset-database.ts",
    "db:validate": "npx tsx scripts/validate-schema.ts",
    "db:studio": "drizzle-kit studio",
    "deploy": "./scripts/deploy.sh",
    "test:build": "npm run typecheck && npm run build",
    "analyze": "ANALYZE=true npm run build",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test-storybook": "test-storybook --watch=false",
    "chromatic": "chromatic --exit-zero-on-changes",
    "chromatic:ci": "chromatic --exit-zero-on-changes --build-script-name storybook:build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:component": "cypress run --component",
    "cy:e2e": "cypress run --e2e",
    "cy:headed": "cypress run --headed --e2e",
    "cy:chrome": "cypress run --e2e --browser chrome",
    "cy:firefox": "cypress run --e2e --browser firefox",
    "cy:edge": "cypress run --e2e --browser edge",
    "cy:cross-browser": "npm run cy:chrome && npm run cy:firefox",
    "test:unit": "NODE_ENV=test vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:ui": "vitest --ui",
    "test:ct": "cypress run --component",
    "test:ct:fast": "cypress run --component --config video=false,screenshotOnRunFailure=false --quiet",
    "test:ct:accessibility": "cypress run --component --config video=false,screenshotOnRunFailure=false --quiet --spec 'app/_lib/**/*.cy.tsx,app/components/auth/**/*.cy.tsx,app/components/ui/**/*.cy.tsx,app/components/layout/**/*.cy.tsx,app/components/branding/**/*.cy.tsx,app/components/pages/**/*.cy.tsx,app/components/examples/**/*.cy.tsx'",
    "test:ct:core": "cypress run --component --config video=false,screenshotOnRunFailure=false --quiet --spec 'app/components/beneficiaries/**/*.cy.tsx,app/components/documents/**/*.cy.tsx,app/components/shared/**/*.cy.tsx,app/components/dashboard/**/*.cy.tsx,app/(dashboard)/**/*.cy.tsx,app/(auth)/**/*.cy.tsx,components/**/*.cy.tsx'",
    "test:ct:parallel": "cypress run --component --config video=false --parallel --record --key $CYPRESS_RECORD_KEY",
    "test:ct:batched": "node scripts/run-tests-batched.js",
    "test:e2e": "cypress run --e2e",
    "test:e2e:headed": "cypress run --headed --e2e",
    "test:all": "pnpm test:unit && pnpm test:ct && pnpm test:e2e",
    "cli": "tsx tools/cli.ts",
    "test:cli": "pnpm cli status && pnpm cli history --limit 5",
    "generate:test": "npx tsx scripts/generate-component-tests.ts",
    "generate:tests:missing": "npx tsx scripts/generate-component-tests.ts --missing-only",
    "generate:tests:upgrade": "npx tsx scripts/generate-component-tests.ts --upgrade",
    "generate:tests:realistic": "npx tsx scripts/realistic-test-generator.ts --overwrite",
    "generate:tests:realistic:missing": "npx tsx scripts/realistic-test-generator.ts --missing-only",
    "generate:tests:realistic:dry-run": "npx tsx scripts/realistic-test-generator.ts --overwrite --dry-run --verbose",
    "test:compliance": "npx tsx scripts/validate-test-compliance.ts",
    "test:compliance:score": "npx tsx scripts/compliance-scorer.ts",
    "lint:tests": "eslint **/*.cy.tsx --no-fix",
    "lint:ai-compliance": "eslint **/*.cy.tsx --no-fix --format=table",
    "generate:tests": "tsx scripts/generate-advanced-tests.ts",
    "generate:tests:watch": "tsx --watch scripts/generate-advanced-tests.ts",
    "generate:api-test": "npx tsx scripts/generate-api-tests.ts",
    "generate:api-tests:missing": "npx tsx scripts/generate-api-tests.ts --missing-only",
    "generate:api-tests:upgrade": "npx tsx scripts/generate-api-tests.ts --upgrade",
    "test:api:compliance": "npx tsx scripts/generate-api-tests.ts --compliance",
    "test:api:coverage": "vitest run --coverage tests/api",
    "test:dashboard": "tsx scripts/test-analytics-dashboard.ts",
    "test:report": "npx tsx scripts/backend-analytics-dashboard.ts",
    "vercel:check": "node vercel-env-check.js",
    "vercel:build-test": "npm run vercel:check && npm run typecheck && npm run build",
    "vercel:test": "node scripts/test-vercel-build.js",
    "vercel:ready": "node scripts/check-vercel-readiness.js",
    "test:backend:ci": "vitest run tests/api --reporter=verbose --coverage",
    "test:gates": "npx tsx scripts/performance-gates.ts",
    "test:monitor": "npx tsx scripts/continuous-performance-monitor.ts",
    "phase3:auth-enhancement": "npx tsx scripts/enhance-authentication-mocking.ts",
    "phase3:typescript-fixes": "npx tsx scripts/fix-test-typescript-errors.ts",
    "phase3:performance-benchmarks": "npx tsx scripts/create-performance-benchmarks.ts",
    "phase3:quality-gates": "npx tsx scripts/performance-gates.ts",
    "phase3:full-optimization": "npm run phase3:typescript-fixes && npm run phase3:auth-enhancement && npm run phase3:performance-benchmarks",
    "test:backend": "vitest run tests/api"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.2",
    "@hookform/resolvers": "^5.2.1",
    "@iconify/react": "^6.0.0",
    "@react-aria/ssr": "3.9.10",
    "@react-aria/visually-hidden": "3.8.26",
    "@react-stately/utils": "^3.10.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@types/dompurify": "^3.0.5",
    "@types/pg": "^8.15.5",
    "@vercel/blob": "^1.1.1",
    "argon2": "^0.44.0",
    "clsx": "2.1.1",
    "dompurify": "^3.2.6",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "framer-motion": "^11.18.2",
    "intl-messageformat": "10.7.16",
    "jose": "^6.0.12",
    "json-rules-engine": "^7.3.1",
    "libphonenumber-js": "^1.12.12",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-themes": "0.4.6",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "signature_pad": "^5.0.10",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@cypress/code-coverage": "^3.14.6",
    "@cypress/react": "^9.0.1",
    "@cypress/webpack-dev-server": "^5.1.1",
    "@eslint/compat": "^1.3.2",
    "@next/bundle-analyzer": "^15.4.6",
    "@next/eslint-plugin-next": "15.4.6",
    "@react-types/shared": "3.30.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "4.1.11",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/cypress": "^1.1.6",
    "@types/node": "20.19.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "chromatic": "^13.1.4",
    "commander": "^14.0.0",
    "cypress": "^15.0.0",
    "cypress-real-events": "^1.14.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "glob": "^11.0.3",
    "globals": "16.0.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "msw": "^2.11.1",
    "msw-storybook-addon": "^2.0.5",
    "postcss": "8.5.6",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.0.13",
    "storybook": "^8.6.14",
    "tailwind-variants": "2.0.1",
