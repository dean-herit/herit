    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4",
    "zod": "^4.0.17"
  },
  "engines": {
    "node": ">=20.18.2",
    "pnpm": ">=9.0.0"
  }
}